{
  "name": "phi-tau",
  "version": "4.0.0",
  "description": "Starter kit for creating apps with React and Redux",
  "scripts": {
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch server:dev lint:watch",
    "postinstall": "NODE_ENV=production npm run clean-dist && npm run build:html && babel-node tools/build.js ",
    "lint": "esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html",
    "build": "NODE_ENV=production babel-node tools/build.js && npm run server:prod",
    "server:dev": "NODE_ENV=development babel-node server/index.js",
    "server:prod": "NODE_ENV=production babel-node server/index.js",
    "test": "mocha tools/testSetup.js \"src/**/*.spec.js\" --reporter spec",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "axios": "0.12.0",
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.5.0",
    "babel-preset-stage-2": "6.13.0",
    "babel-register": "6.8.0",
    "baffle": "0.3.4",
    "bcrypt": "0.8.7",
    "body-parser": "1.15.2",
    "chai": "3.5.0",
    "cheerio": "0.20.0",
    "connect-history-api-fallback": "1.2.0",
    "css-loader": "0.23.1",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.22.0",
    "jsonwebtoken": "7.0.1",
    "lodash": "4.13.1",
    "mongoose": "4.5.2",
    "morgan": "1.7.0",
    "node-phonenumber": "0.2.3",
    "node-sass": "3.7.0",
    "object-assign": "4.1.0",
    "react": "15.0.2",
    "react-count-to": "0.4.2",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.5",
    "react-script-loader": "0.0.1",
    "redux": "3.5.2",
    "redux-form": "5.3.2",
    "redux-react-router": "1.0.0-beta3",
    "redux-thunk": "2.1.0",
    "rethinkdb": "2.3.2",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "stripe": "4.10.0",
    "style-loader": "0.13.1",
    "sweetalert": "1.1.3",
    "sweetalert-react": "0.4.4",
    "twilio": "2.9.1",
    "webpack": "1.13.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "devDependencies": {
